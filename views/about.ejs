<%- include('header') %>
<!-- Body start -->
<div class="row mb-2">
    <div class="col bg-primary text-white">
        <h2><%= title %></h2>
    </div>
</div>
<div class="row mb-5">
    
    <div class="col">
        <h2>Item list :</h2>
        <% items.forEach(data=>{ %>
            <ul>
                <li>
                    <%= data %>
                </li>
            </ul>
            <% })%>
        </div>
        <div class="col-6 mx-5">
            <caption>
                <center>
                    <h2>Users List</h2>
                </center>
            </caption>
            <table class="table table-striped">
                <thead>
                    <th>Name</th>
                    <th>Age</th>
                    <th>City</th>
                </thead>
                <% users.forEach(data=>{ %>
                    <tr>
                        <td>
                            <%= data.name%>
                        </td>
                        <td>
                            <%= data.age%>
                        </td>
                        <td>
                            <%= data.city%>
                        </td>
                    </tr>
                    <% })%>
                </table>
            </div>
        </div>

<!-- Body End -->
        
